        <!--
        Account Journal
        -->

        <record id="diario_venta" model="account.journal">
            <field name="name">Diario de Ventas</field>
            <field name="code">venta</field>
            <field name="type">sale</field>
            <field name="view_id" ref="account.account_journal_view"/>
            <field name="sequence_id" ref="account.sequence_sale_journal"/>
            <field model="account.account" name="default_credit_account_id" ref="a_vtp"/>
            <field model="account.account" name="default_debit_account_id" ref="a_vtp"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="diario_compra" model="account.journal">
            <field name="name">Diario de Compras</field>
            <field name="code">compra</field>
            <field name="type">purchase</field>
            <field name="view_id" ref="account.account_journal_view"/>
