            <field model="account.account" name="default_debit_account_id" ref="a_cmp"/>
            <field model="account.account" name="default_credit_account_id" ref="a_cmp"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <record id="diario_banco" model="account.journal">
            <field name="name">Diario de Banco</field>
            <field name="code">banco</field>
            <field name="type">cash</field>
