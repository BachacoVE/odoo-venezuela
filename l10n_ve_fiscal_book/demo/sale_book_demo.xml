            <field name="fb_id" ref="fiscal_book_sale_test"/>
            <!--
                <field name="fblt_ids" ref=""/>  #### Tax Lines one2many
            -->
            <field name="get_credit_affected"> credit_affected SX00 </field>
            <field name="get_date_imported" eval="time.strftime('%Y-%m-%d')"/>
            <field name="get_date_invoiced" eval="time.strftime('%Y-%m-%d')"/>
            <field name="get_debit_affected"> debit_affected SX00</field>
            <field name="get_doc">Trans. Type SX00</field>
            <field name="get_number">CtrlN SX00</field>
            <field name="get_parent">CtrlN SX00</field>
            <field name="get_partner_name">Partner SX00</field>
            <field name="get_partner_vat">VAT SX00</field>
            <field name="get_reference">Inv Nro. SX00</field>
            <field name="get_t_doc">Doc. Type SX00</field>
            <!--
                <field name="invoice_id" ref=""/> Invoice many2one 
                <field name="iwdl_id" ref=""/> Vat Withholding many2one 
