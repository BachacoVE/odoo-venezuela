        <!--    Fiscal Book     -->

        <record id="fiscal_book_sale_test" model="fiscal.book">
            <field name="name">Sale Book (Report Look&amp;Feel test - Dont Update Lines)</field>
            <field name="type">sale</field>
            <field name="period_id" search="[]" model="account.period"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="note">Really large entry test</field>
        </record>

        <!--    Fiscal Book Taxes    -->

