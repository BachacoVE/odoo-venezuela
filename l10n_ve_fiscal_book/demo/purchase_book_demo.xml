<openerp>
    <data>

        <!--    Fiscal Book     -->

        <record id="fiscal_book_purchase_test" model="fiscal.book">
            <field name="name">Purchase Book (Report Look&amp;Feel test - Dont Update Lines)</field>
            <field name="type">purchase</field>
            <field name="period_id" search="[]" model="account.period"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="note">Really large entry test</field>
        </record>

        <!--    Fiscal Book Taxes    -->

        <!--    Fiscal Book Lines    -->

        <record id="fiscal_book_line_purchase_1" model="fiscal.book.lines">
            <field name="rank" eval="1"/>
            <field name="fb_id" ref="fiscal_book_purchase_test"/>
            <!--
                <field name="fblt_ids" ref=""/>  #### Tax Lines one2many
            -->
            <field name="get_credit_affected"> credit_affected PX1 </field>
            <field name="get_date_imported" eval="time.strftime('%Y-%m-%d')"/>
            <field name="get_date_invoiced" eval="time.strftime('%Y-%m-%d')"/>
            <field name="get_debit_affected"> debit_affected PX1</field>
            <field name="get_doc">Trans. Type PX1</field>
            <field name="get_number">CtrlN PX1</field>
            <field name="get_parent">CtrlN PX1</field>
            <field name="get_partner_name">Partner PX1</field>
            <field name="get_partner_vat">VAT PX1</field>
            <field name="get_reference">Inv Nro. PX1</field>
            <field name="get_t_doc">Doc. Type PX1</field>
            <!--
                <field name="invoice_id" ref=""/> Invoice many2one 
                <field name="iwdl_id" ref=""/> Vat Withholding many2one 
            -->
        </record>

