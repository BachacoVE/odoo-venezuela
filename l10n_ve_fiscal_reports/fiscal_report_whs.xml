            <field name="model">fiscal.reports.whs</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Detail of entry">
                    <group col="6" colspan="4">
                        <field name="ai_date_invoice"/>
                        <field name="rp_vat"/>
                        <field name="rp_id"/>
                        <field name="ai_nro_ctrl"/>
                        <field name="ai_number"/>
                        <field name="ai_amount_total"/>
                        <field name="ai_amount_untaxed"/>
                        <field name="ai_amount_tax"/>
                        <field name="ai_type"/>
                        <field name="rp_retention"/>
                        <field name="ai_id"/>
                    </group>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="view_fiscal_reports_whs_tree">
            <field name="name">fiscal.reports.whs.tree</field>
            <field name="model">fiscal.reports.whs</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="List of Purchases." editable="top/bottom">
                        <field name="ai_date_invoice"/>
                        <field name="rp_vat"/>
                        <field name="rp_id"/>
                        <field name="ai_nro_ctrl"/>
                        <field name="ai_number"/>
                        <field name="ai_amount_total"/>
                        <field name="ai_amount_untaxed"/>
                        <field name="ai_amount_tax"/>
                        <field name="ai_type"/>
                        <field name="rp_retention"/>
                        <field name="ai_id"/>
                </tree>
            </field>
        </record>
    <record id="action_fiscal_reports_whs_tree" model="ir.actions.act_window">
    <field name="name">Profit by Product</field>
    <field name="res_model">fiscal.reports.whs</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="menu_fiscal_reports_whs_tree" name="Sale Witholding Book" parent="l10n_ve_fiscal_reports.menu_fr_main" action="action_fiscal_reports_whs_tree"/>
    </data>
</openerp>
